<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE token  [
    <!ENTITY style SYSTEM "shared.css">
    <!ENTITY about.en SYSTEM "about.en.js">
    <!ENTITY approve.en SYSTEM "approve.en.js">
]>
<ts:token
    xmlns:ts="http://tokenscript.org/2020/06/tokenscript"
    xmlns:xhtml="http://www.w3.org/1999/xhtml"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:asnx="urn:ietf:params:xml:ns:asnx"
    xmlns:ethereum="urn:ethereum:constantinople"
    xsi:schemaLocation="http://tokenscript.org/2020/06/tokenscript http://tokenscript.org/2020/06/tokenscript.xsd"
    custodian="false"
>
    <ts:label>
        <ts:string xml:lang="en">GFO</ts:string>
    </ts:label>
    <ts:contract name="contractWithEventTransfer">
        <ts:address network="4">0x8289Fe21203987911E9F67cCe514A681D3A1883c</ts:address>
        <asnx:module name="Transfer">
            <sequence>
                <element name="_from" ethereum:type="address" ethereum:indexed="true"/>
                <element name="_to" ethereum:type="address" ethereum:indexed="true"/>
                <element name="_value" ethereum:type="uint256" ethereum:indexed="false"/>
            </sequence>
        </asnx:module>
    </ts:contract>
    <ts:contract name="contractWithEventApproval">
        <ts:address network="4">0x8289Fe21203987911E9F67cCe514A681D3A1883c</ts:address>
        <asnx:module name="Approval">
            <sequence>
                <element name="_owner" ethereum:type="address" ethereum:indexed="true"/>
                <element name="_spender" ethereum:type="address" ethereum:indexed="true"/>
                <element name="_value" ethereum:type="uint256" ethereum:indexed="false"/>
            </sequence>
        </asnx:module>
    </ts:contract>
    <ts:contract interface="erc20" name="GFO">
        <ts:address network="4">0x8289Fe21203987911E9F67cCe514A681D3A1883c</ts:address>
        <!--mainnet-->
    </ts:contract>
    <ts:origins>
        <!-- Define the contract which holds the token that the user will use -->
        <ts:ethereum contract="GFO"/>
        <!-- as above ts:contract name -->
    </ts:origins>
    <ts:selection id="isAdmin" filter="admin=0x0D590124d2fAaBbbdFa5561ccBf778914a50BCca">
        <ts:label>
            <ts:string xml:lang="en">Must be the admin</ts:string>
        </ts:label>
    </ts:selection>
    <ts:cards>

        <ts:card type="action" exclude="isAdmin">
            <ts:label>
                <ts:string xml:lang="en">Move</ts:string>
            </ts:label>

            <ts:attribute-type name="from" syntax="1.3.6.1.4.1.1466.115.121.1.36">
                <ts:label>
                    <ts:string xml:lang="en">to address</ts:string>
                </ts:label>
                <ts:origins>
                    <ts:user-entry as="address"/>
                </ts:origins>
            </ts:attribute-type>

            <ts:attribute-type name="to" syntax="1.3.6.1.4.1.1466.115.121.1.36">
                <ts:label>
                    <ts:string xml:lang="en">to address</ts:string>
                </ts:label>
                <ts:origins>
                    <ts:user-entry as="address"/>
                </ts:origins>
            </ts:attribute-type>

            <ts:attribute-type name="amount" syntax="1.3.6.1.4.1.1466.115.121.1.36">
                <ts:label>
                    <ts:string xml:lang="en">amount</ts:string>
                </ts:label>
                <ts:origins>
                    <ts:user-entry as="e8"/>
                </ts:origins>
            </ts:attribute-type>

            <ts:transaction>
                <ethereum:transaction function="transferFrom" contract="GFO" as="uint">
                    <ts:data>
                        <ts:address ref="from"/>
                        <ts:address ref="to"/>
                        <ts:uint256 ref="amount"/>
                    </ts:data>
                </ethereum:transaction>
            </ts:transaction>
            <ts:view
                xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
                <style type="text/css">&style;</style>
                <script type="text/javascript">&approve.en;</script>
            </ts:view>
        </ts:card>

        <ts:card type="action" exclude="isAdmin">
            <ts:label>
                <ts:string xml:lang="en">Add</ts:string>
            </ts:label>
            <ts:attribute-type name="addAddress" syntax="1.3.6.1.4.1.1466.115.121.1.36">
                <ts:label>
                    <ts:string xml:lang="en">address to add to whitelist</ts:string>
                </ts:label>
                <ts:origins>
                    <ts:user-entry as="address"/>
                </ts:origins>
            </ts:attribute-type>
            <ts:transaction>
                <ethereum:transaction function="approve" contract="GFO" as="uint">
                    <ts:data>
                        <ts:address ref="approvalAddress"/>
                        <ts:uint256>115792089237316195423570985008687907853269984665640564039457584007913129639935</ts:uint256>
                    </ts:data>
                </ethereum:transaction>
            </ts:transaction>
            <ts:view
                    xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
                <style type="text/css">&style;</style>
                <script type="text/javascript">&approve.en;</script>
            </ts:view>
        </ts:card>

        <ts:card type="action" exclude="isAdmin">
            <ts:label>
                <ts:string xml:lang="en">Remove</ts:string>
            </ts:label>
            <ts:attribute-type name="removalAddress" syntax="1.3.6.1.4.1.1466.115.121.1.36">
                <ts:label>
                    <ts:string xml:lang="en">removal address from whitelist</ts:string>
                </ts:label>
                <ts:origins>
                    <ts:user-entry as="address"/>
                </ts:origins>
            </ts:attribute-type>
            <ts:transaction>
                <ethereum:transaction function="approve" contract="GFO" as="uint">
                    <ts:data>
                        <ts:address ref="approvalAddress"/>
                        <ts:uint256>115792089237316195423570985008687907853269984665640564039457584007913129639935</ts:uint256>
                    </ts:data>
                </ethereum:transaction>
            </ts:transaction>
            <ts:view
                    xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
                <style type="text/css">&style;</style>
                <script type="text/javascript">&approve.en;</script>
            </ts:view>
        </ts:card>
    </ts:cards>

    <ts:attribute-type name="admin" syntax="1.3.6.1.4.1.1466.115.121.1.15">
        <ts:label>
            <ts:string xml:lang="en">admin</ts:string>
        </ts:label>
        <ts:origins>
            <ethereum:call function="admin" contract="GFO" as="address"/>
        </ts:origins>
    </ts:attribute-type>

    <ts:attribute-type name="decimals" syntax="1.3.6.1.4.1.1466.115.121.1.36">
        <ts:label>
            <ts:string xml:lang="en">decimals</ts:string>
        </ts:label>
        <ts:origins>
            <ethereum:call function="decimals" contract="GFO" as="uint"/>
        </ts:origins>
    </ts:attribute-type>

    <ts:attribute-type name="name" syntax="1.3.6.1.4.1.1466.115.121.1.26">
        <ts:label>
            <ts:string xml:lang="en">name</ts:string>
        </ts:label>
        <ts:origins>
            <ethereum:call function="name" contract="GFO" as="utf8"/>
        </ts:origins>
    </ts:attribute-type>

    <ts:attribute-type name="symbol" syntax="1.3.6.1.4.1.1466.115.121.1.26">
        <ts:label>
            <ts:string xml:lang="en">symbol</ts:string>
        </ts:label>
        <ts:origins>
            <ethereum:call function="symbol" contract="GFO" as="utf8"/>
        </ts:origins>
    </ts:attribute-type>

    <ts:attribute-type name="totalSupply" syntax="1.3.6.1.4.1.1466.115.121.1.36">
        <ts:label>
            <ts:string xml:lang="en">totalSupply</ts:string>
        </ts:label>
        <ts:origins>
            <ethereum:call function="totalSupply" contract="GFO" as="uint"/>
        </ts:origins>
    </ts:attribute-type>

</ts:token>
